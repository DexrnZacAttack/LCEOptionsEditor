<UserControl x:Class="LCEOptionsEditor.ControlBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:LCEOptionsEditor"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ComboBox x:Name="ctrlBox" SelectionChanged="CtrlBox_OnSelectionChanged" Width="200" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center"></ComboBox>
    </Grid>
</UserControl>
